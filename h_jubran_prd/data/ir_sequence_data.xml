<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sequence_petty_cash" model="ir.sequence">
        <field name="name">Petty Cash Sequence</field>
        <field name="code">h_jubran.petty.cash</field>
        <field name="prefix">PC-</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <!-- Purchase Request Sequence -->
    <record id="sequence_purchase_request" model="ir.sequence">
        <field name="name">Purchase Request</field>
        <field name="code">h_jubran.purchase.request</field>
        <field name="prefix">PR</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>


