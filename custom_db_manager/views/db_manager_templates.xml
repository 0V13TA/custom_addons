<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="web.login_layout_custom" inherit_id="web.login">
        <!-- This is not used directly in DB selector, but good practice -->
    </template>

    <!-- Override the DB manager template -->
    <template id="web.database_manager_logo" inherit_id="web.database_manager">
        <xpath expr="//img[@src='/web/static/src/img/logo.svg']" position="attributes">
            <attribute name="src">/custom_db_manager/static/src/img/logo.svg</attribute>
            <attribute name="alt">EnterpriseOne</attribute>
        </xpath>
    </template>
</odoo>