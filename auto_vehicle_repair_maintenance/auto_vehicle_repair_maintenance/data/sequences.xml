<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Inspection -->
        <record id="seq_inspection" model="ir.sequence">
            <field name="name">Inspection Sequence</field>
            <field name="code">auto.vehicle.repair.inspection</field>
            <field name="prefix">INS</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <!-- Sequence for Work Order -->
        <record id="seq_work_order" model="ir.sequence">
            <field name="name">Work Order Sequence</field>
            <field name="code">auto.vehicle.repair.work.order</field>
            <field name="prefix">WO</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <!-- Sequence for Maintenance Record -->
        <record id="seq_maintenance_record" model="ir.sequence">
            <field name="name">Maintenance Record Sequence</field>
            <field name="code">auto.vehicle.repair.maintenance</field>
            <field name="prefix">MR</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>

